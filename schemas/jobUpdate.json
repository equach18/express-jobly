{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "examples": [
                "New"
            ]
        },
        "salary": {
            "title": "The salary Schema",
            "type": "integer",
            "minimum": 0,
            "examples": [
                3785634
            ]
        },
        "equity": {
            "title": "The equity Schema",
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "examples": [
                0.01
            ]
        },
        "companyHandle": {
            "title": "The companyHandle Schema",
            "type": "string",
            "examples": [
                "c1"
            ]
        }
    },
    "examples": [
        {
            "title": "New",
            "salary": 3785634,
            "equity": 0.01,
            "companyHandle": "c1"
        }
    ]
}